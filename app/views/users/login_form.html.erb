<% provide(:title, "Log In") %>
<% provide(:form_title, "Log In") %>

<h1>Log In</h1>

<%= form_with(url: login_path, local: true) do |f| %>

  <%= f.label :Username %>
  @<%= f.text_field :user_name %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= link_to("Forgot Password", new_password_reset_url) %>
  <br>
  <%= f.submit "Log In" %>
<% end %>
